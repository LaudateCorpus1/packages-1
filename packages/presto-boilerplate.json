    "android": "./node_modules/cordova/bin/cordova run android",
    "ios": "./node_modules/cordova/bin/cordova run ios",
    "web": "babel-node ./runner/webServer.js",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "postinstall": "npm run build-mobile"
  },
