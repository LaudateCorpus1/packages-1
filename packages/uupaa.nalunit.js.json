{
  "name": "uupaa.nalunit.js",
  "version": "0.0.13",
  "description": "MPEG4-15 AVC(H.264) - NAL file format",
  "url": "https://github.com/uupaa/NALUnit.js",
  "keywords": [
    "AVC",
    "H.264",
    "NALUnit",
    "AUD",
    "EBSP",
    "IDR",
