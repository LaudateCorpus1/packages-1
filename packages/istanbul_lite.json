    "publish-alias": "VERSION=$(npm info $npm_package_name version); for ALIAS in istanbul_lite; do utility2 shRun shNpmPublishAs . $ALIAS $VERSION; utility2 shRun shNpmTestPublished $ALIAS || exit $?; done",
