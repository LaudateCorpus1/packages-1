    "dts-generator-tf": "^1.7.0-beta.0",
    "eslint": "^2.2.0",
    "path-sort": "^0.1.0",
    "plist": "^1.2.0",
    "proxyquire": "^1.7.4",
    "publish-please": "^1.1.0",
    "read-package-json": "^2.0.3",
    "rimraf": "^2.5.2",
    "should": "^8.2.2",
