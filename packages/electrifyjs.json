{
  "name": "electrifyjs",
  "version": "0.5.4",
  "description": "A scaffolding tool for making desktop apps from websites.",
  "main": "dist/index.js",
  "files": [
    "src",
    "template",
    "dist"
  ],
  "scripts": {
    "lint": "xo",
    "precommit": "yarn lint",
    "prepush": "yarn lint",
    "start": "babel ./src -d ./dist -w",
    "build": "babel ./src -d ./dist",
    "clean": "rimraf ./dist",
    "prepublish": "yarn lint && yarn run clean && yarn run build"
  },
  "bin": {
    "electrify": "dist/index.js"
  },
  "repository": {
