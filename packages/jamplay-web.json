  "scripts": {
    "patch": "patch-package && echo ****patched extracts file pls remove this if extract file support blob",
    "postinstall": "yarn patch && cd lib/jamplay-validator && yarn install",
    "start": "NODE_ENV=production node entry.js",
    "release": "standard-version --release&&node ./bin/replace-issue-link",
    "release-patch": "standard-version --release-as patch&&node ./bin/replace-issue-link",
    "build-comment": "sh bin/build-comment.bash",
    "release-rc": "standard-version --release-as patch --prerelease rc&&node ./bin/replace-issue-link",
    "alpha-deploy": "sh bin/alpha-deploy.bash&&sh bin/alpha-up.bash",
    "test": "jest",
    "dev": "BABEL_ENV=server nodemon --exec babel-node -w ./server -w ./entry.js -w ./routes entry.js",
    "e2e": "NODE_ENV=test jest --config ./e2e/config.json",
    "next": "next",
    "clean": "bash ./bin/clean.bash",
    "build": "bash ./bin/build.bash",
    "storybook": "start-storybook -p 9001 -s ./ -c .storybook",
    "docker:build-alpha": "docker build -f Dockerfile -t reg.jamplay.world/jamplay/jamplay-web-alpha:$(node -p \"require('./package.json').version\") . && docker tag reg.jamplay.world/jamplay/jamplay-web-alpha:$(node -p \"require('./package.json').version\") reg.jamplay.world/jamplay/jamplay-web-alpha:latest",
    "docker:build-beta": "docker build -f Dockerfile -t reg.jamplay.world/jamplay/jamplay-web-beta:$(node -p \"require('./package.json').version\") . && docker tag reg.jamplay.world/jamplay/jamplay-web-beta:$(node -p \"require('./package.json').version\") reg.jamplay.world/jamplay/jamplay-web-beta:latest",
    "docker:build-local": "docker build -f Dockerfile -t reg.jamplay.world/jamplay/jamplay-web-local:$(node -p \"require('./package.json').version\") . && docker tag reg.jamplay.world/jamplay/jamplay-web-local:$(node -p \"require('./package.json').version\") reg.jamplay.world/jamplay/jamplay-web-local:latest",
    "docker:push-image-alpha": "docker push reg.jamplay.world/jamplay/jamplay-web-alpha",
    "docker:push-image-beta": "docker push reg.jamplay.world/jamplay/jamplay-web-beta",
    "docker:push-image-local": "docker push reg.jamplay.world/jamplay/jamplay-web-local",
    "imagemin": "NODE_ENV=development node bin/imagemin.js"
  },
