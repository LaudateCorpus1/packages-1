  "browser": "cjs/cordova.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "node ./watch.js",
    "hcp": "cd test && cordova-hcp server",
    "android": "cd test && cordova run android --device ; cd ..",
    "ios": "cd test && cordova run ios --device ; cd .. && ios_webkit_debug_proxy",
    "build_serialport": "cp -r ./node_modules/serialport ./node_modules/serialport_electron && cd ./node_modules/serialport_electron && node-gyp rebuild --target=1.4.4 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "build_noble": "cp -r ./node_modules/noble ./node_modules/noble_electron && cd ./node_modules/noble_electron && npm install;",
    "doc_util": "node ~/work/scratch/joi-to-markdown/bin.js > api.md"
  },
