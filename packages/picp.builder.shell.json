  "scripts": {
    "start": "electron .",
    "install": "node install.js",
    "test": "mocha --reporter spec",
    "buildosx": "electron-packager app picp.builder --app-version=$npm_package_version --platform=darwin --arch=all --version=1.4.1 --overwrite --out=dist --icon=app/icon.icns",
    "buildwin": "electron-packager app picp.builder --app-version=1.1.0 --platform=win32 --arch=x64 --version=1.4.1 --overwrite --out=dist --icon=app/icon.ico",
    "pack": "build --dir",
    "dist": "build --win --x64",
    "publish": "node publish && build --win --x64"
  },
