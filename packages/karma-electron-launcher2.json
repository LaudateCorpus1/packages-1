    "jshint": "~2.5.10",
    "karma": "~0.13.21",
    "karma-mocha": "~0.2.2",
    "mocha": "~1.11.0",
    "twolfson-style": "~1.6.0",
    "xtend": "~4.0.1"
  },
  "keywords": [
    "karma-plugin",
    "karma-launcher",
    "karma-framework",
    "karma-adapter",
    "electron"
  ],
  "foundry": {
