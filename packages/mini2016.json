  "main": "index.js",
  "description": "browser ui for electron, written in react",
  "scripts": {
    "pkg": "electron-packager ./ mini2016 --platform=win32 --arch=x64 --out ./OutApp --version 1.4.7 --overwrite --icon=build/icon.ico",
