  "bin": {
    "susi": "app/main/index.js"
  },
  "scripts": {
    "start": "electron app",
    "test": "xo",
    "pack": "electron-builder",
    "dist": "electron-builder --dir"
  },
  "repository": {
    "user": "fossasia",
    "repo": "susi_desktop",
