{
  "name": "newslynx-electron",
  "productName": "Newslynx",
  "version": "0.0.1",
  "description": "A desktop version of the NewsLynx web app.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newslynx/newslynx-electron.git"
  },
  "author": {
    "name": "mhkeller",
