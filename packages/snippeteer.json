    "clone_url": "https://github.com/imRish/snippeteer",
    "https_url": "https://github.com/imRish/snippeteer",
    "travis_url": "https://travis-ci.org/imRish/snippeteer",
    "zip_url": "https://github.com/imRish/snippeteer/archive/master.zip",
    "api_url": "https://api.github.com/repos/imRish/snippeteer"
  },
  "keywords": [
    "productivity",
