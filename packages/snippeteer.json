  "build": {
    "appId": "snippeteer",
    "mac": {
      "category": "public.app-category.productivity",
      "extendInfo": {
        "LSBackgroundOnly": 1,
        "LSUIElement": 1
      }
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
