{
  "name": "btron",
  "version": "0.2.0",
  "description": "Bundle Electron projects with live reload",
  "main": "./build/index.js",
  "bin": {
    "btron": "bin/btron"
  },
  "scripts": {
    "prebuild": "mkdir -p build",
    "build": "babel --ignore=node_modules,_* src --out-dir build",
    "dev": "npm run build -s -- --watch",
    "clean": "rm -rf build",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
