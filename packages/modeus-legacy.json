    "//bump": "bunp version and publish tag and commit, npm run bump -- -ver x.x.x",
    "bump": "powershell -Command ./bump.ps1",
    "//build": "build lib",
    "build": "powershell -Command ./build.ps1",
    "//dist": "sync built lib with link folder (../modeus-core-dist)",
    "dist": "(robocopy ./dist ../modeus-legacy-dist /MIR /NFL /NDL /NJH /NJS /nc /ns /np) ^& exit 0",
    "//bdist": "build + dist",
    "bdist": "npm run build && npm run dist",
    "//release": "tag build version and optionally publish to npm, npm run release -- -ver x.x.x",
    "release": "powershell -Command ./release.ps1",
    "//pub": "build + release (with publishing)",
    "pub": "npm run build && npm run pub -- -skipPublish 0",
    "//build-lib-server": "build lib on server via docker",
    "build-lib-server": "sh build-lib.sh"
