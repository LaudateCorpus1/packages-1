  "main": "library/index.js",
  "scripts": {
    "build": "npm run build-clear && npm run build-library",
    "build-clear": "dr-js -m rm -a ./library",
    "build-library": "cross-env NODE_ENV=production BABEL_ENV=library babel ./source --out-dir ./library --copy-files",
    "build-library-dev": "cross-env NODE_ENV=development BABEL_ENV=library-dev babel ./source --out-dir ./library --copy-files --watch",
    "prepack": "npm run build",
    "publish-latest": "npm publish --userconfig ~/thatbean.npmrc",
    "publish-dev": "npm publish --tag dev --userconfig ~/thatbean.npmrc"
  },
